# platform specifies which version were you working on.
platform :ios, '13.0'
target 'Mozemoz' do
  use_frameworks!
 
 # Pods for Firebase
 pod 'Firebase/Core'
 pod 'Firebase/Auth'
 # Pods for Google Sign In.
 pod 'GoogleSignIn'
 # Pods for Facebook Sign In.
 pod 'FacebookCore'
 pod 'FacebookLogin'
 pod 'FacebookShare'
 
end

